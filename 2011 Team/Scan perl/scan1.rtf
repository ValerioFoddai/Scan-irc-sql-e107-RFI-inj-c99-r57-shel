sub isFound() \{\
    my $status = 0;\
    my $link = $_[0];\
    my $reqexp = $_[1];\
    my $res = &get_content($link);\
    if ($res =~ /$reqexp/) \{ $status = 1 \}\
    return $status;\
\}\
\
sub get_content() \{\
    my $url = $_[0];\
    my $ua = LWP::UserAgent->new(agent => $uagent);\
    $ua->timeout(7);\
    my $req = HTTP::Request->new(GET => $url);\
    my $res = $ua->request($req);\
    return $res->content;\
\}\
