sub search_engine_query() \{\
    my $url = $_[0];\
    $url =~ s/http:\\/\\///;\
    my $host = $url;\
    my $query = $url;\
    my $page  = "";\
    $host =~ s/href=\\"?http:\\/\\///;\
    $host =~ s/([-a-zA-Z0-9\\.]+)\\/.*/$1/;\
    $query =~ s/$host//;\
    if ($query eq "") \{ $query = "/"; \}\
        eval \{\
            my $sock = IO::Socket::INET->new(PeerAddr=>"$host", PeerPort=>"80", Proto=>"tcp") or return;\
            print $sock "GET $query HTTP/1.0\\r\\nHost: $host\\r\\nAccept: */*\\r\\nUser-Agent: $uagent\\r\\n\\r\\n";\
            my @pages = <$sock>;\
            $page = "@pages";\
            close($sock);\
        \};\
    return $page;\
\}\
